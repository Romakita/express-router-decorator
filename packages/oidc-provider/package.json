    "@tsed/di": "^6.49.1",
    "@tsed/exceptions": "^6.49.1",
    "oidc-provider": "^7.3.0"
  },
  "peerDependencies": {
    "@tsed/common": "^6.48.6",
    "@tsed/core": "^6.48.6",
    "@tsed/di": "^6.48.6",
    "@tsed/schema": "^6.48.6",
    "@tsed/json-mapper": "^6.48.6",
    "@tsed/exceptions": "^6.48.6",
