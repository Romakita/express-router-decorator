{
  "name": "@tsed/oidc-provider",
  "version": "6.49.1",
  "description": "OIDC provider package for Ts.ED framework",
  "private": false,
  "keywords": [
    "tsed",
    "oidc",
    "openid",
    "oauth2",
    "express",
    "koa",
    "security",
    "authentication"
