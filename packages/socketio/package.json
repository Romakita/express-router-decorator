    "@tsed/openspec": "^6.49.1",
    "express": "^4.17.1",
    "socket.io": "4.0.1",
    "socket.io-client": "4.0.1"
  },
  "peerDependencies": {
    "@tsed/common": "^6.48.6",
    "@tsed/core": "^6.48.6",
    "@tsed/di": "^6.48.6",
    "@tsed/schema": "^6.48.6",
    "@tsed/json-mapper": "^6.48.6",
