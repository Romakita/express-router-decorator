  "dependencies": {
    "ajv-formats": "1.5.1",
    "tslib": "2.2.0"
  },
  "devDependencies": {
    "@tsed/core": "6.49.1",
    "@tsed/di": "6.49.1",
    "@tsed/exceptions": "6.49.1",
    "@tsed/schema": "6.49.1",
    "ajv": "8.3.0"
  },
  "peerDependencies": {
    "@tsed/core": "^6.18.0",
    "@tsed/di": "^6.18.0",
    "@tsed/exceptions": "^6.18.0",
    "@tsed/schema": "^6.18.0",
    "ajv": "7||8"
  },
